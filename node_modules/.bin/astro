#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/astro@5.13.5_@netlify+blobs_be0b8d6d0a5d657bc75ebc771b845830/node_modules/astro/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/astro@5.13.5_@netlify+blobs_be0b8d6d0a5d657bc75ebc771b845830/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/astro@5.13.5_@netlify+blobs_be0b8d6d0a5d657bc75ebc771b845830/node_modules/astro/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/astro@5.13.5_@netlify+blobs_be0b8d6d0a5d657bc75ebc771b845830/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
