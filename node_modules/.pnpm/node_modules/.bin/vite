#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules/vite/bin/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules/vite/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules/vite/bin/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules/vite/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/vite@6.3.5_@types+node@24.3_1a9fdf8331bba68fe013986905e2e6a2/node_modules:/mnt/d/Proyectos/Congreso/Web/RCH-HELP/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
