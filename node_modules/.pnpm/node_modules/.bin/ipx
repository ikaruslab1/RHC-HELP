#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules/ipx/bin/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules/ipx/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules/ipx/bin/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules/ipx/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/ipx@3.1.1_@netlify+blobs@10.0.10/node_modules:/mnt/c/Proyectos/RHC-HELP/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ipx/bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../ipx/bin/ipx.mjs" "$@"
fi
